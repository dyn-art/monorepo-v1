{
  "name": "physicaldotart",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start:dev": "turbo run start:dev",
    "clean": "turbo run clean",
    "install:clean": "turbo run install:clean",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "dc:build": "docker-compose build --progress=plain --no-cache",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [],
  "author": "@bennodev19",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.8.7",
    "shx": "^0.3.4",
    "turbo": "^1.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.3.1"
}
