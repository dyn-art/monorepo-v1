{
  "name": "@pda/figma-plugin",
  "version": "0.0.0",
  "description": "Figma Plugin",
  "private": true,
  "main": "code.js",
  "scripts": {
    "build": "shx rm -rf dist && webpack --config webpack.prod.js",
    "build:dev": "shx rm -rf dist && webpack --config webpack.dev.js",
    "start:dev": "webpack --watch --progress --config webpack.dev.js",
    "lint": "eslint --cache \"**/*.{js,ts}\"",
    "clean": "shx rm -rf dist && shx rm -rf node_modules && shx -rf .turbo",
    "install:clean": "pnpm run clean && pnpm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "@bennodev19",
  "license": "MIT",
  "dependencies": {
    "@pda/figma-handler": "workspace:*",
    "@pda/logger": "workspace:*",
    "@pda/s3": "workspace:^",
    "clsx": "^1.2.1",
    "daisyui": "^2.51.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.64.0",
    "@pda/tsconfig": "workspace:*",
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-pda-base": "workspace:*",
    "html-inline-script-webpack-plugin": "^3.1.0",
    "html-webpack-plugin": "^5.5.1",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.2.4",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.1.0",
    "webpack-merge": "^5.8.0"
  }
}
