{
  "name": "@pda/figma-plugin",
  "version": "0.0.0",
  "description": "Figma Plugin",
  "private": true,
  "scripts": {
    "build": "shx rm -rf dist && rollup --config rollup.config.js --bundleConfigAsCjs --prod",
    "build:dev": "shx rm -rf dist && rollup --config rollup.config.js --bundleConfigAsCjs",
    "preview": "concurrently \"pnpm run watch:dev:preview\" \"pnpm run start:preview-server\" \"pnpm run storybook\"",
    "watch:dev:preview": "shx rm -rf dist && rollup --config rollup.config.js --bundleConfigAsCjs --watch --preview",
    "start:preview-server": "npx esno ./develop/preview-server/index.ts",
    "storybook": "storybook dev -p 6006",
    "lint": "eslint --cache \"**/*.{js,ts}\"",
    "clean": "shx rm -rf dist && shx rm -rf node_modules && shx rm -rf .turbo",
    "install:clean": "pnpm run clean && pnpm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "@bennodev19",
  "license": "MIT",
  "dependencies": {
    "@agile-ts/core": "^0.2.9",
    "@pda/core-client": "workspace:*",
    "@pda/figma-handler": "workspace:*",
    "@pda/figma-to-dtif": "workspace:*",
    "@pda/google-client": "workspace:^",
    "@pda/logger": "workspace:*",
    "@pda/openapi-fetch": "workspace:*",
    "@pda/utils": "workspace:*",
    "clsx": "^1.2.1",
    "crypto-js": "^4.1.1",
    "daisyui": "^3.0.20",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-element-to-jsx-string": "^15.0.0",
    "react-json-tree": "^0.18.0",
    "react-router-dom": "^6.11.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.64.0",
    "@pda/tsconfig": "workspace:*",
    "@pda/types": "workspace:*",
    "@rollup/plugin-html": "^1.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@storybook/addon-essentials": "7.0.18",
    "@storybook/addon-interactions": "7.0.18",
    "@storybook/addon-links": "7.0.18",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "7.0.18",
    "@storybook/react": "7.0.18",
    "@storybook/react-vite": "7.0.18",
    "@storybook/testing-library": "0.0.14-next.2",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.13",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/ws": "^8.5.4",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-config-pda-base": "workspace:*",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss-jsx": "^0.0.1",
    "express": "^4.18.2",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.2.4",
    "prop-types": "15.8.1",
    "storybook": "7.0.18",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "ws": "^8.13.0"
  }
