{
  "name": "@pda/renderer",
  "version": "0.0.0",
  "description": "Utility library to work with Music and communicate with Spotify or Genius",
  "private": true,
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run bundle && shx rm -rf dist && tsc",
    "bundle": "shx rm -rf bundle && esno ./scripts/bundle",
    "start:dev": "tsc -w",
    "start:remotion": "remotion preview ./src/ui/src/index.ts --config ./src/ui/remotion.config.ts",
    "lint": "eslint --cache \"**/*.{js,ts}\"",
    "clean": "shx rm -rf bundle && shx rm -rf dist && shx rm -rf node_modules && shx -rf .turbo",
    "install:clean": "pnpm run clean && pnpm install",
    "test": "jest"
  },
  "keywords": [],
  "author": "@bennodev19",
  "license": "MIT",
  "dependencies": {
    "@remotion/renderer": "^3.3.76"
  },
  "devDependencies": {
    "@pda/jest-presets": "workspace:^0.0.0",
    "@pda/tsconfig": "workspace:*",
    "@remotion/bundler": "^3.3.76",
    "@remotion/cli": "^3.3.76",
    "@types/jest": "^29.4.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/web": "^0.0.99",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.4",
    "axios-hooks": "^4.0.0",
    "clsx": "^1.2.1",
    "css-loader": "^6.7.3",
    "d3": "^7.8.2",
    "d3-geo": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-config-pda-base": "workspace:*",
    "geojson": "^0.5.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remotion": "^3.3.76",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.5.2",
    "webpack": "^5.76.1"
  },
  "jest": {
    "preset": "@pda/jest-presets/jest/node"
  },
  "files": [
    "dist",
    "bundle",
    "README.md"
  ]
}
