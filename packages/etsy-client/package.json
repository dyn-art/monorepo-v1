{
  "name": "@pda/etsy-client",
  "version": "0.0.0",
  "description": "Client library to communicate with the Etsy API",
  "private": true,
  "source": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "shx rm -rf dist && npx esno ../../scripts/build-ts-package.ts --prod",
    "start:dev": "tsc -w",
    "generate:openapi": "npx openapi-typescript ./resources/openapi-3.json -o ./src/gen/v3.ts",
    "lint": "eslint --cache \"**/*.{js,ts}\"",
    "clean": "shx rm -rf dist && shx rm -rf node_modules && shx rm -rf .turbo",
    "install:clean": "pnpm run clean && pnpm install",
    "test": "jest"
  },
  "keywords": [],
  "author": "@bennodev19",
  "license": "MIT",
  "dependencies": {
    "@pda/client-utils": "workspace:*",
    "@pda/logger": "workspace:*"
  },
  "devDependencies": {
    "@pda/jest-presets": "workspace:^",
    "@pda/tsconfig": "workspace:*",
    "@types/jest": "^29.5.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-config-pda-base": "workspace:*",
    "jest": "^29.5.0",
    "openapi-typescript": "^6.2.8",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
